   "source": [
    "def pow(a, b):\n",
    "    c = a\n",
    "    res = c\n",
    "    for i in range(b-1):\n",
    "        c = res\n",
    "        for j in range(a-1):\n",
    "            res += c\n",
    "    return res"
   ]
